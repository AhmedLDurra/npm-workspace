{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-daily-gantt-chart": [
        "dist/ngx-daily-gantt-chart/ngx-daily-gantt-chart",
        "dist/ngx-daily-gantt-chart"
      ],
      "ngx-icon-calendar": [
        "dist/ngx-icon-calendar/ngx-icon-calendar",
        "dist/ngx-icon-calendar"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}